{
  "id": "concrete-volume-calculator",
  "slug": "concrete-volume-calculator",
  "category": "construction",
  "engine": "function",
  "calculationId": "calculateConcreteVolume",
  "tags": ["concrete", "volume", "concrete-volume", "construction"],
  "inputs": [
    {
      "name": "shape",
      "type": "select",
      "options": [
