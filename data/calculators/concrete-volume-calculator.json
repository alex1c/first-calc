      "defaultValue": "false"
    },
    {
      "name": "wastePercent",
      "type": "number",
      "min": 5,
      "max": 15,
      "step": 1,
      "defaultValue": 10,
      "visibleIf": {
        "field": "includeWaste",
        "value": "true"
      }
    }
  ],
  "outputs": [
    {
      "name": "volume"
    },
    {
      "name": "volumeWithWaste"
    },
    {
      "name": "volumeUnit"
    },
    {
      "name": "volumeUnitFull"
    },
    {
      "name": "formula"
    },
    {
      "name": "shapeName"
    },
    {
      "name": "explanation"
    }
