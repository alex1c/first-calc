	"outputs": [
		{
			"name": "l100km"
		},
		{
			"name": "mpgUS"
		},
		{
			"name": "mpgUK"
		},
		{
			"name": "convertedValue"
		},
		{
			"name": "insights"
		}
	],
	"defaults": {
		"mode": "calculate",
		"distance": 500,
		"distanceUnit": "km",
		"fuelUsed": 35,
		"fuelUnit": "liters"
