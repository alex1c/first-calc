			"visibleIf": {
				"field": "mode",
				"value": "calculate"
			}
		},
		{
			"name": "fuelUsed",
			"type": "number",
			"step": 1,
			"defaultValue": 8,
			"visibleIf": {
				"field": "mode",
				"value": "calculate"
			}
		},
		{
			"name": "fuelUnit",
			"type": "select",
			"options": [
				{ "value": "liters", "label": "Liters (L)" },
				{ "value": "gallons us", "label": "Gallons US" },
				{ "value": "gallons uk", "label": "Gallons UK" }
			],
			"defaultValue": "liters",
			"visibleIf": {
				"field": "mode",
				"value": "calculate"
			}
		},
		{
			"name": "value",
			"type": "number",
			"step": 1,
			"visibleIf": {
				"field": "mode",
				"value": "convert"
			}
		},
		{
			"name": "fromUnit",
			"type": "select",
			"options": [
				{ "value": "L/100km", "label": "Liters per 100 km (L/100km)" },
				{ "value": "MPG US", "label": "Miles per Gallon US (MPG US)" },
				{ "value": "MPG UK", "label": "Miles per Gallon UK (MPG UK)" }
			],
			"defaultValue": "L/100km",
			"visibleIf": {
				"field": "mode",
				"value": "convert"
			}
		},
		{
			"name": "toUnit",
			"type": "select",
			"options": [
				{ "value": "L/100km", "label": "Liters per 100 km (L/100km)" },
