      "name": "pilesCount",
      "type": "number",
      "min": 1,
      "max": 10000,
      "step": 1
    },
    {
      "name": "pileDiameter",
      "type": "number",
      "min": 0,
      "max": 1000000000,
      "step": "any"
    },
    {
      "name": "pileDepth",
      "type": "number",
      "min": 0,
      "max": 1000000000,
      "step": "any"
    },
    {
      "name": "pileCapCount",
      "type": "number",
      "min": 0,
      "max": 10000,
      "step": 1,
      "defaultValue": 0,
      "optional": true
    },
    {
      "name": "pileCapLength",
      "type": "number",
      "min": 0,
      "max": 1000000000,
      "step": "any",
      "optional": true
    },
    {
      "name": "pileCapWidth",
      "type": "number",
      "min": 0,
      "max": 1000000000,
      "step": "any",
      "optional": true
    },
    {
      "name": "pileCapHeight",
      "type": "number",
      "min": 0,
      "max": 1000000000,
      "step": "any",
      "optional": true
    },
    {
      "name": "unit",
      "type": "select",
      "options": [
        { "value": "meters", "label": "Meters (m)" },
        { "value": "feet", "label": "Feet (ft)" }
      ],
      "defaultValue": "meters"
    },
    {
      "name": "includeWaste",
      "type": "select",
      "options": [
        { "value": "false", "label": "No" },
        { "value": "true", "label": "Yes" }
      ],
      "defaultValue": "false"
    },
    {
      "name": "wasteMargin",
      "type": "number",
      "min": 5,
      "max": 20,
      "step": 1,
      "defaultValue": 10,
      "visibleIf": {
        "field": "includeWaste",
        "value": "true"
      }
