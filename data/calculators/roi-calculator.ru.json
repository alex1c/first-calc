{
  "id": "roi-calculator",
  "category": "finance",
  "slug": "roi-calculator",
  "inputs": [
    {
      "name": "initialInvestment",
      "type": "number",
      "unit": "₽",
      "min": 0
    },
    {
      "name": "finalValue",
      "type": "number",
      "unit": "₽",
      "min": 0
    }
  ],
  "outputs": [
    {
      "name": "roi"
    },
    {
      "name": "profit"
    }
  ],
  "formula": "(function() { const profit = finalValue - initialInvestment; const roi = (profit / initialInvestment) * 100; return { roi: roi, profit: profit }; })()",
  "variables": {
    "initialInvestment": "Сумма начальных инвестиций",
    "finalValue": "Текущая или конечная стоимость инвестиций"
  },
  "relatedIds": [
    "compound-interest",
    "inflation-adjustment",
    "loan-payment"
  ],
  "standardIds": []
}



