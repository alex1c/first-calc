{
	"id": "savings-calculator",
	"slug": "savings-calculator",
	"category": "finance",
	"engine": "function",
	"calculationId": "calculateSavings",
	"tags": ["finance", "savings", "investments", "compound-interest", "retirement"],
	"inputs": [
		{
			"name": "initialSavings",
			"type": "number",
			"min": 0,
			"max": 10000000,
			"step": 100,
			"defaultValue": 5000
		},
		{
			"name": "monthlyContribution",
			"type": "number",
			"min": 0,
			"max": 100000,
			"step": 1,
			"defaultValue": 200
		},
		{
			"name": "interestRate",
			"type": "number",
			"min": 0,
			"max": 20,
			"step": 0.01,
			"defaultValue": 5
		},
		{
			"name": "years",
			"type": "number",
			"min": 1,
			"max": 100,
			"step": 1,
			"defaultValue": 10
		},
		{
			"name": "compoundingFrequency",
			"type": "select",
			"options": [
				{ "value": "monthly", "label": "Monthly" },
				{ "value": "quarterly", "label": "Quarterly" },
				{ "value": "annually", "label": "Annually" }
			],
			"defaultValue": "monthly"
		},
		{
			"name": "interestType",
			"type": "select",
			"options": [
				{ "value": "compound", "label": "Compound Interest" },
				{ "value": "simple", "label": "Simple Interest" }
			],
			"defaultValue": "compound"
		},
		{
			"name": "inflationRate",
			"type": "number",
			"min": 0,
			"max": 20,
			"step": 0.1,
			"defaultValue": 0
		},
		{
			"name": "taxRate",
			"type": "number",
			"min": 0,
			"max": 50,
			"step": 0.1,
			"defaultValue": 0
		},
		{
			"name": "monthlyWithdrawal",
			"type": "number",
			"min": 0,
			"max": 100000,
			"step": 1,
			"defaultValue": 0
		}
	],
	"outputs": [
		{
			"name": "finalValue"
		},
		{
			"name": "totalContributions"
		},
		{
			"name": "totalInterest"
		},
		{
			"name": "growthPercentage"
		},
		{
			"name": "realValue"
		},
		{
			"name": "afterTaxValue"
		},
		{
			"name": "steps"
		}
	],
	"defaults": {
		"initialSavings": 5000,
		"monthlyContribution": 200,
		"interestRate": 2.5,
		"years": 10,
		"compoundingFrequency": "monthly"
	},
	"isEnabled": true
}

